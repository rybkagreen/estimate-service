{
  "default": true,
  "MD001": true,
  "MD003": {
    "style": "atx"
  },
  "MD004": {
    "style": "dash"
  },
  "MD007": {
    "indent": 2
  },
  "MD010": {
    "spaces_per_tab": 2
  },
  "MD013": false,
  "MD022": {
    "lines_above": 1,
    "lines_below": 1
  },
  "MD024": {
    "allow_different_nesting": true
  },
  "MD025": {
    "front_matter_title": "^\\s*title\\s*[:=]"
  },
  "MD026": {
    "punctuation": ".,;:!。，；：！"
  },
  "MD029": {
    "style": "ordered"
  },
  "MD033": {
    "allowed_elements": [
      "br",
      "sub",
      "sup",
      "kbd",
      "img",
      "details",
      "summary",
      "div",
      "span",
      "em",
      "strong",
      "code",
      "pre"
    ]
  },
  "MD035": {
    "style": "---"
  },
  "MD036": false,
  "MD041": false,
  "MD046": {
    "style": "fenced"
  },
  "MD048": {
    "style": "backtick"
  }
}
